<odoo>
    <record id="view_livestock_category_tree" model="ir.ui.view">
        <field name="name">livestock.category.tree</field>
        <field name="model">livestock.category</field>
        <field name="arch" type="xml">
            <list editable="bottom">
                <field name="name"/>
                <field name="active"/>
            </list>
        </field>
    </record>

    <record id="action_livestock_category" model="ir.actions.act_window">
        <field name="name">Categor√≠as</field>
        <field name="res_model">livestock.category</field>
        <field name="view_mode">list</field>
    </record>

    <record id="view_livestock_breed_tree" model="ir.ui.view">
        <field name="name">livestock.breed.tree</field>
        <field name="model">livestock.breed</field>
        <field name="arch" type="xml">
            <list editable="bottom">
                <field name="name"/>
                <field name="active"/>
            </list>
        </field>
    </record>

    <record id="action_livestock_breed" model="ir.actions.act_window">
        <field name="name">Razas</field>
        <field name="res_model">livestock.breed</field>
        <field name="view_mode">list</field>
    </record>

    <record id="view_livestock_location_tree" model="ir.ui.view">
        <field name="name">livestock.location.tree</field>
        <field name="model">livestock.location</field>
        <field name="arch" type="xml">
            <list editable="bottom">
                <field name="name"/>
                <field name="active"/>
            </list>
        </field>
    </record>

    <record id="action_livestock_location" model="ir.actions.act_window">
        <field name="name">Ubicaciones / Lotes</field>
        <field name="res_model">livestock.location</field>
        <field name="view_mode">list</field>
    </record>
</odoo>
